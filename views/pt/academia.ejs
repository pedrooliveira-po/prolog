    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const estreia = new Date('2025-10-01');
            const agora = new Date();
            if(agora < estreia) {
                document.querySelector('.coming-soon span').style.display = 'none';
            } else {
                const badge = document.querySelector('.badge');
                if (badge) {
                badge.parentNode.removeChild(badge);
                }
            }
        })
        
        document.querySelector('#hamburguer').addEventListener('click', () => {
            document.querySelector('header').classList.toggle('menu')
            if(document.querySelector('header').classList.contains('menu')) {
                document.querySelector('#navMenu').classList.remove('hidden');
                document.querySelector('#navMenu').classList.remove('block');
            } else {
                document.querySelector('#navMenu').classList.remove('block');
                document.querySelector('#navMenu').classList.remove('hidden');
            }
        })
        const imagens = document.querySelectorAll('img');
        
        imagens.forEach((imagem) => {
            imagem.addEventListener('click', () => {
                const divImagem = document.createElement('div');
                divImagem.className = 'w-full h-full bg-black/80 fixed inset-0 z-10000 flex items-center justify-center';
                
                const clone = imagem.cloneNode(true);
                clone.className = 'max-w-full max-h-[90vh] object-contain';
                divImagem.append(clone);
                
                divImagem.addEventListener('click', (e) => {
                    if (e.target === divImagem) {
                        divImagem.remove();
                    }
                });
        
                document.body.prepend(divImagem);
            });
        });

        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) {
                    target.scrollIntoView({
                        behavior: 'smooth',
                        block: 'start'
                    });
                }
            });
        });

        const observerOptions = {
            threshold: 0.1,
            rootMargin: '0px 0px -50px 0px'
        };

        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, observerOptions);

        document.querySelectorAll('.mvv-card, .course-card, .methodology-item, .benefit-item').forEach(el => {
            el.style.opacity = '0';
            el.style.transform = 'translateY(20px)';
            el.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
            observer.observe(el);
        });
    </script>
